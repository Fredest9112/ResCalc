<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ResistorCalc" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <!-- @color/purple_500 -->
        <item name="colorPrimary">#388e3c</item>
        <!-- @color/purple_700 -->
        <item name="colorPrimaryVariant">#6abf69</item>
        <!-- @color/white -->
        <item name="colorOnPrimary">#000000</item>
        <!-- Secondary brand color. -->
        <!-- @color/teal_200 -->
        <item name="colorSecondary">#6C6767</item>
        <!-- @color/teal_700 -->
        <item name="colorSecondaryVariant">#ffffff</item>
        <!-- @color/black -->
        <item name="colorOnSecondary">#000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <!-- Radio buttons -->
        <item name="radioButtonStyle">@style/Widget.TipTime.CompoundButton.RadioButton</item>
    </style>
</resources>